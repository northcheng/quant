{
    "data_source": {
        "api_key": {
            "alpha_vantage": ""
        },
        "time_col": {
            "yahoo": "Date",
            "yfinance": "Date"
        },
        "interval_val": {
            "day": {
                "yahoo": "d",
                "yfinance": "1d"
            },
            "week": {
                "yahoo": "w",
                "yfinance": "1wk"
            },
            "month": {
                "yahoo": "m",
                "yfinance": "1mo"
            }
        }
    },
    "calculation": {
        "file_name": {
            "ta_data": "ta_data.pkl",
            "result": "result.pkl",
            "final_result": "final_result.pkl"
        },
        "update_sec_data": true,
        "update_ta_data": true,
        "save_sec_data": true,
        "look_back_window": {
            "day": 300,
            "week": 1260,
            "month": 5400
        }
    },
    "visualization": {
        "create_image": true,
        "show_image": true,
        "save_image": false,
        "plot_window": {
            "day": 200,
            "week": 1260,
            "month": 5400
        },
        "bar_width": {
            "day": 0.62,
            "week": 3.72,
            "month": 15.5
        },
        "plot_args": {
            "plot_ratio": {
                "signals": 0.75,
                "main_indicators": 2.5,
                "volume": 0.75
            },
            "adx": {
                "target_col": [
                    "adx",
                    "pdi",
                    "mdi"
                ],
                "benchmark": 20
            },
            "aroon": {},
            "aroon_gap_sda": {
                "target_col": [
                    "aroon_sda"
                ],
                "benchmark": 0,
                "color_mode": "benchmark"
            },
            "cci": {
                "target_col": [
                    "cci"
                ],
                "benchmark": 0,
                "color_mode": "benchmark"
            },
            "dpo": {
                "target_col": [
                    "dpo"
                ],
                "benchmark": 0,
                "color_mode": "benchmark"
            },
            "eom": {
                "target_col": [
                    "eom_diff"
                ],
                "benchmark": 0,
                "color_mode": "benchmark"
            },
            "main_indicators": {
                "target_indicator": [
                    "candlestick",
                    "ichimoku",
                    "renko",
                    "linear"              
                ],
                "candlestick_width": 1,
                "candlestick_color": {
                    "colorup": "green",
                    "colordown": "red",
                    "alpha": 1
                }
            },
            "kst": {
                "target_col": [
                    "kst_diff"
                ],
                "benchmark": 0,
                "color_mode": "up_down"
            },
            "mi": {
                "target_col": [
                    "mi"
                ]
            },
            "psar": {
                "target_col": [
                    "psar_up_indicator",
                    "psar_down_indicator"
                ],
                "benchmark": 0,
                "color_mode": "benchmark"
            },
            "renko": {
                "plot_in_date": true
            },
            "rsi": {
                "target_col": [
                    "rsi"
                ],
                "boundary": [
                    30,
                    70
                ]
            },
            "signals": {
                "signal_list": [
                    "bb_signal",
                    "renko_signal",
                    "psar_signal",
                    "aroon_signal",
                    "adx_signal",
                    "ichimoku_signal",
                    "signal"
                ],
                "plot_signal_on_price": true,
                "price_alpha": 0
            },
            "other": {
                "target_col": [
                    "rsi"
                ]
            },
            "trend_idx": {
                "target_col": [
                    "up_trend_idx",
                    "down_trend_idx",
                    "trend_idx"
                ],
                "benchmark": 0
            },
            "volume": {
                "target_col": "Volume",
                "color_mode": "up_down",
                "bar_width": 1,
                "alpha": 0.8
            }
        }
    },
    "postprocess": {
        "keep_columns": {
            "symbol": "代码",
            "Close": "收盘",
            "rate": "涨跌",
            "acc_rate": "累计",
            "obos": "超买超卖",
            "break_up": "上穿",
            "break_down": "下穿",
            "close_to_kijun": "基准",
            "close_to_tankan": "转换",
            "ichimoku_day": "ICHI",
            "support": "支撑",
            "resistant": "阻挡",
            "category": "类别",
            "description": "描述",
            "signal": "交易信号"
        },
        "drop_columns": [],
        "save_result": true,
        "save_data": true,
        "extract_signal_image": true,
        "extract_portfolio_image": true,
        "zip_ta_model_folder": false
    },
    "trade": {
        "init_cash": {
            "tiger":{
                "real": 2000,
                "simu": 10000
            },
            "futu":{
                "real": 2000,
                "simu": 10000
            },
            "simulation_account": 10000,
            "global_account": 2000,
            "standard_account": 2000,
            "SIMULATE": 10000,
            "REAL": 2000
        },
        "pool": {
            "tiger": {
                "real": "tiger_pool",
                "simu": "universe"
            },
            "futu":{
                "real": "futu_pool",
                "simu": "universe"
            },
            "simulation_account": "universe",
            "global_account": "tiger_pool",
            "standard_account": "tiger_pool",
            "SIMULATE": "universe",
            "REAL": "futu_pool"
        },
        "stop_profit": {
            "tiger":{
                "real": 10,
                "simu": 10
            },
            "futu":{
                "real": 10,
                "simu": 10
            }
        },
        "stop_loss": {
            "tiger":{
                "real": -0.1,
                "simu": -0.5
            },
            "futu":{
                "real": -0.1,
                "simu": -0.5
            }
        }
    }
}